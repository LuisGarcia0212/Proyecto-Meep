<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/fondoMeep"
    tools:context=".actividades.AcercaDeActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_margin="15dp"
        android:orientation="vertical"
        tools:layout_editor_absoluteX="13dp"
        tools:layout_editor_absoluteY="26dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <ImageView
                android:id="@+id/backCuenta"
                android:layout_width="50dp"
                android:layout_height="30dp"
                android:src="@drawable/ic_flechaback"
                android:layout_marginTop="10dp"/>

            <ImageView
                android:id="@+id/imageView"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentStart="true"
                android:layout_marginTop="35dp"
                android:layout_marginLeft="50dp"
                android:src="@drawable/logo1" />

            <TextView
                android:id="@+id/lblProyecto"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="10dp"
                android:layout_marginTop="45dp"
                android:layout_toEndOf="@id/imageView"
                android:fontFamily="@font/inter_regular"
                android:gravity="center_vertical"
                android:text="@string/lblProyecto"
                android:textColor="#FFFFFF"
                android:textSize="35sp" />

        </RelativeLayout>

        <RelativeLayout
            android:layout_width="365sp"
            android:layout_height="530sp"
            android:layout_marginTop="40dp"
            android:background="@drawable/border1">

            <TextView
                android:id="@+id/lblAcercaDe1"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_regular"
                android:text="@string/lblAcercaDe1"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                android:layout_marginTop="10dp"
                android:layout_marginLeft="60dp"/>




            <TextView
                android:id="@+id/lblAcercaDe"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:fontFamily="@font/inter_regular"
                android:text="@string/lblAcercaDe"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:layout_marginTop="50dp"
                android:layout_marginLeft="10dp"/>



        </RelativeLayout>





    </LinearLayout>






</androidx.constraintlayout.widget.ConstraintLayout>